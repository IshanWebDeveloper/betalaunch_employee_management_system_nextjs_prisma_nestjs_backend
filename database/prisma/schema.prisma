// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "foreignKeys"
}

model Employees {
  EmployeeID    Int           @id
  Name          String        @unique
  Designation   String
  Experience    Int
  Employee_Type employee_type @default(Other)
}

enum employee_type {
  Full_Time
  Part_Time
  Contract_Basis
  Other
}
